{
  "name": "@mytiki/tiki-capture-receipt-capacitor",
  "version": "0.6.0",
  "description": "Capture receipts w/ TIKI!",
  "main": "dist/tiki-receipt-capacitor.umd.js",
  "module": "dist/tiki-receipt-capacitor.es.js",
  "type": "module",
  "types": "dist/esm/index.d.ts",
  "unpkg": "dist/tiki-receipt-capacitor.umd.js",
  "exports": {
    ".": {
      "types": "./dist/types/main.d.ts",
      "import": "./dist/tiki-receipt-capacitor.es.js"
    },
    "./dist/tiki-receipt-capacitor.css": "./dist/tiki-receipt-capacitor.css"
  },
  "files": [
    "android/src/main/",
    "android/build.gradle",
    "dist/",
    "ios/Plugin/",
    "MytikiTikiCaptureReceiptCapacitor.podspec"
  ],
  "author": "Tiki inc.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tiki/tiki-capture-receipt-capacitor.git"
  },
  "bugs": {
    "url": "https://github.com/tiki/tiki-capture-receipt-capacitor/issues"
  },
  "keywords": [
    "capacitor",
    "plugin",
    "tiki",
    "microblink"
  ],
  "scripts": {
    "verify": "npm run verify:ios && npm run verify:android && npm run verify:web",
    "verify:ios": "cd ios && pod deintegrate && pod install && xcodebuild -workspace Plugin.xcworkspace -scheme Plugin -destination generic/platform=iOS && cd ..",
    "verify:android": "cd android && ./gradlew clean build test && cd ..",
    "verify:web": "npm run build",
    "lint": "npm run eslint && npm run prettier -- --check && npm run swiftlint -- lint",
    "fmt": "npm run eslint -- --fix && npm run prettier -- --write && npm run swiftlint -- --fix --format",
    "eslint": "eslint \"src/**/*.{ts,js}\"",
    "prettier": "prettier \"src/**/*.{css,html,ts,js,java}\"",
    "swiftlint": "node-swiftlint",
    "build": "npm run clean && tsc && vite build",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/types": "^7.23.0",
    "@capacitor/android": "^5.4.1",
    "@capacitor/core": "^5.4.1",
    "@capacitor/docgen": "^0.2.1",
    "@capacitor/ios": "^5.4.1",
    "@ionic/prettier-config": "^4.0.0",
    "@ionic/swiftlint-config": "^1.1.2",
    "@types/node": "^20.7.1",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "rollup-plugin-typescript2": "^0.35.0",
    "swiftlint": "^1.0.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.5.2"
  },
  "peerDependencies": {
    "@capacitor/android": "^5.4.1",
    "@capacitor/core": "^5.4.1",
    "@capacitor/ios": "^5.4.1"
  },
  "prettier": "@ionic/prettier-config",
  "swiftlint": "@ionic/swiftlint-config",
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  }
}
